@font-face {
  font-family: 'WysiBBiconFont';
  src: url('theme/fonts/wysibbiconfont-wb.eot');
  src: url('theme/fonts/wysibbiconfont-wb') format('woff'),
     url('theme/fonts/wysibbiconfont-wb.ttf') format('truetype'),
     url(data:application/x-font-woff;charset=utf-8;base64,) format('woff');
  font-weight: normal;
  font-style: normal;
}

// Color variables (appears count calculates by raw css)
@color0: #aaaaaa; // Appears 24 times
@color1: #ffffff; // Appears 15 times
@color2: #cccccc; // Appears 14 times
@color3: #eeeeee; // Appears 9 times
@color4: #333333; // Appears 7 times
@color5: #dddddd; // Appears 7 times
@color6: #428bca; // Appears 4 times
@color7: #bbbbbb; // Appears 3 times

// Width variables (appears count calculates by raw css)
@width5: 20px; // Appears 23 times


