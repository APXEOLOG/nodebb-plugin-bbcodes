#wbbmodal {
  -webkit-overflow-scrolling: touch;
  background: rgba(0,0,0,0.5);
  bottom: 0;
  font: 12px/1.2 Arial,Verdana;
  left: 0;
  overflow: auto;
  position: fixed;
  right: 0;
  text-align:center;
  top: 0;
  z-index: 1100;
  zoom: 1;
  button {
    *display:inline;
    display:inline-block;
    font-size:14px;
    padding:3px 15px;
    zoom: 1;
    &:focus {
      outline: 0;
    }
    &:active {
      outline: 0;
    }
  }
  .wbbm {
    background: @color1;
    border: 1px solid @color7;
    box-shadow:0px 0px 5px @color4;
    margin: 30px auto;
    max-height:90%;
    max-width: 90%;
    overflow:auto;
    text-align:left;
    .wbbm-tablist {
      padding: 10px 20px;
      ul {
        list-style-type:none;
        margin:0;
        padding:0;
        li {
          border-radius: 0px;
          cursor:pointer;
          display:inline-block;
          margin: 0 10px 5px 0;
          margin: 5px 0;
          padding:10px;
          position:relative;
          &:hover {
            background:@color3;
          }
        }
        li.on {
          background:@color6;
          color:@color1;
          cursor:default;
          &:hover {
            background:@color6;
            color:@color1;
            cursor:default;
          }
        }
      }
    }
  }
  .wbbm-title {
    border-bottom:1px solid @color2;
    color:@color4;
    padding:5px 10px;
    position:relative;
    .wbbm-title-text {
      font-size:1.6em;
      line-height:2em;
      margin:0;
      padding:0;
    }
    .wbbclose {
      color:#888888;
      cursor:pointer;
      display:block;
      font-size:21px;
      font-weight:bold;
      position:absolute;
      right:15px;
      text-transform:uppercase;
      top:15px;
      &:hover {
        color:red;
      }
    }
  }
  .wbbm-cont {
    padding:10px 20px;
  }
  .wbbm-content {
    max-height:100%;
    min-height:50px;
  }
  .div-modal-text {
    border:1px solid @color0;
    font-size:14px;
    line-height: 28px;
    max-height:100px;
    overflow:auto;
    padding:2px 5px;
  }
  .wbbm-bottom {
    background:#f1f1f1;
    border-top:1px solid @color2;
    padding:10px;
  }
  .wbb-button {
    background:@color6;
    border-radius:0px;
    border:1px solid #357ebd;
    color:@color1;
    cursor:pointer;
    line-height:26px;
    &:hover {
      background:#3276b1;
      border:1px solid #285e8e;
    }
  }
  .wbb-cancel-button {
    background:#f5f5f5;
    border-radius:0px;
    border:1px solid @color5;
    color:@color4;
    cursor:pointer;
    line-height:26px;
    margin-left:15px;
    &:hover {
      background:@color1;
      border:1px solid @color2;
    }
  }
  .wbb-remove-button {
    background:#d9534f;
    border-radius:0px;
    border:1px solid #d43f3a;
    color:@color1;
    cursor:pointer;
    float:right;
    line-height:26px;
    &:hover {
      background:#d2322d;
      border:1px solid #ac2925;
    }
  }
  .wbbm-inp-row {
    margin-bottom:15px;
    label {
      display:block;
      font-weight:bold;
      margin-bottom:3px;
    }
    input {
      border-radius:0;
      border:1px solid @color0;
      box-sizing:border-box;
      display:block;
      font-size:14px;
      height:34px;
      line-height: 1.42857143;
      outline:none;
      padding:2px 5px;
      width:100%;
    }
  }
  .wbbm-inperr {
    color:red;
    display:block;
    font-size:10px;
  }
  .wbbm-brdred {
    border-color:red !important;
  }
  #imguploader {
    text-align: center;
  }
  #imguploader.drag {
    border: 3px dashed @color2;
    .p {
      color: @color0;
      font-size:2em;
      margin-top: 15px;
    }
    .p2 {
      color: @color0;
    }
    .fileupload {
      margin: 15px 0;
    }
  }
  #imguploader.drag.dragover {
    background:#feffe4;
  }
  #imguploader.drag.wbb-loading {
    padding:40px 0;
  }
  .fileupload {
    margin: 40px 0;
    position: relative;
    input.file {
      display: block;
      filter: alpha(opacity=0);
      height: 32px !important;
      left: 50%;
      margin-left: -115px;
      opacity: 0;
      position: absolute;
      top: 2px;
      width: 230px;
    }
  }
  .dragupload {
    margin: 10px 0 15px 0;
    position: relative;
  }
}
