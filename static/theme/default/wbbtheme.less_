      .wysibb-toolbar-btn.wbb-select.on {
        background:@color2;
        border:1px solid @color0;
        box-shadow:inset 0px 0px 3px @color0;
        padding: 1px 1px 1px 5px;
        &:active {
          background:@color2;
          border:1px solid @color0;
          box-shadow:inset 0px 0px 3px @color0;
